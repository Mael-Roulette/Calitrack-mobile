{
  "expo": {
    "name": "Calitrack",
    "slug": "calitrack-ts",
    "version": "0.9.5",
    "orientation": "portrait",
    "icon": "./assets/images/logo-fond.png",
    "scheme": "calitrackts",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFF9F7"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.mael_rltt.calitrackts",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFF9F7",
          "dark": {
            "backgroundColor": "#FFF9F7"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application nécessite l'accès à vos photos pour que vous puissiez choisir une image de profil."
        }
      ],
      "expo-notifications",
      "@react-native-firebase/app",
      "expo-audio",
      "expo-asset",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_ROUTER_APP_ROOT": "./app",
      "router": {},
      "eas": {
        "projectId": "aa0ea6c8-f82d-4087-a86b-4e88c39288c4"
      }
    }
  }
}
